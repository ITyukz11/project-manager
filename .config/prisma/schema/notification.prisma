model Notifications {
  id            String   @id @default(cuid())
  userId        String
  user          User     @relation(fields: [userId], references: [id])
  isRead        Boolean
  link          String?
  message       String
  type          String?
  actor       String?  
  subject     String?  
  casinoGroup String? 
  createdAt     DateTime @default(now())
  updatedAt     DateTime @updatedAt @map(name: "updated_at")
}