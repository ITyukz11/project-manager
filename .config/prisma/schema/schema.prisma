generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("PRISMA_DATABASE_URL") // uses connection pooling
}

enum Role {
  ADMIN
  FAP
  MASTER_AGENT
  TL
  LOADER
  ACCOUNTING    
}


model GroupChat {
  id          String      @id @default(cuid())
  name        String
  status      Boolean     @default(true)

  users       NetworkUser[]     
  createdAt   DateTime    @default(now())
  updatedAt   DateTime    @updatedAt
}