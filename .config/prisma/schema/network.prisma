
model GroupChat {
  id          String      @id @default(cuid())
  name        String @unique
  status      Boolean     @default(true)
  users       User[]    
  casinoGroup CasinoGroup @relation(fields: [casinoGroupId], references: [id])
  casinoGroupId String
  createdAt   DateTime    @default(now())
  updatedAt   DateTime    @updatedAt
}